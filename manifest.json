{
  "background": {
    "scripts": [ "background.js" ]
  },
  "description": "Remove iframe headers",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "Chrome No iFrame",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.linkedin.com/*",
    "*://us.linkedin.com/*",
    "*://ca.linkedin.com/*",
    "*://twitter.com/*",
    "*://www.twitter.com/*"
  ],
  "version": "0.1"
}
